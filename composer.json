{
    "name": "archette/appgen",
    "description": "Nette code generator to speed up development",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rixafy",
            "email": "rixafy@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "nette/neon": "^3.1",
        "nette/schema": "^1.0",
        "nette/php-generator": "^3.3",
        "symfony/console": "^5.0"
    },
    "bin": [
        "bin/appgen",
        "bin/appgen.phar"
    ],
    "autoload": {
        "psr-4": {
